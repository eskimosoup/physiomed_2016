<div class="hero-section standard-page-background">
  <div class="hero-section-header">
    <h1 class="hero-section-title page full-width">
      Find a Physio
    </h1>
  </div>

  <div class="hero-section-primary-content">
    <div class="page-content-wrap full-width">
      <div class="page-content">
      <div class="physio-searches">
        <%= render(
              partial: "detailed_physio_searches/form",
              locals:
              {
                physio_search: @physio_search
              }
            ) %>
      </div>
      </div>
    </div>

    <div class="page-contact-methods-wrap">
      <%= render(
        partial: 'detailed_physio_searches/shared/sidebar',
        locals: {
          call_to_action: true,
          testimonials: nil,
          team_members: nil,
          health_zone: wellbeing_zone_health_zone,
          case_studies: nil,
          articles: nil,
        },
        cache: wellbeing_zone_health_zone
      ) %>
    </div>
  </div>
</div>
