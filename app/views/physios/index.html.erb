<div class="hero-section standard-page-background">
  <div class="hero-section-header">
    <h1 class="hero-section-title page full-width">
      Physio Network - Videos
    </h1>
  </div>

  <div class="hero-section-primary-content">
    <div class="page-content-wrap full-width">
      <div class="page-content">
        <div>
          <h2>Initial Assessment Video</h2>
          <iframe class='sproutvideo-player' src='//videos.sproutvideo.com/embed/e89bdfb5141ceccb60/bc3ff9b49120835f' width='630' height='354' frameborder='0' allowfullscreen></iframe>
          <div>
            <%= image_tag 'word_logo.png', title: 'Word', alt: 'Word' %>
            <%= link_to 'Download Initial Assessment Report - Annotated macros', "#{ root_url }#{ URI.encode('Initial Assessment Report - Annotated macros.docx') }", class: 'button brown-background' %>
          </div>
        </div>

        <div>
          <h2>Initial Report Video</h2>
          <iframe class='sproutvideo-player' src='//videos.sproutvideo.com/embed/a49bdfb5141cecc42c/5cb1cf02e74d3fe3' width='630' height='354' frameborder='0' allowfullscreen></iframe>
          <div>
            <%= image_tag 'word_logo.png', title: 'Word', alt: 'Word' %>
            <%= link_to 'Download Interim Report - Annotated macros', "#{ root_url }#{ URI.encode('Interim Report - Annotated macros.docx') }", class: 'button brown-background' %>
          </div>
        </div>

        <div>
          <h2>Discharge Report Video</h2>
          <iframe class='sproutvideo-player' src='//videos.sproutvideo.com/embed/4c9bdfb5141cecc6c4/d06be6f25955f904' width='630' height='354' frameborder='0' allowfullscreen></iframe>
          <div>
            <%= image_tag 'word_logo.png', title: 'Word', alt: 'Word' %>
            <%= link_to 'Download Discharge Report - Annotated macros', "#{ root_url }#{ URI.encode('Discharge Report - Annotated macros.docx') }", class: 'button brown-background' %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
