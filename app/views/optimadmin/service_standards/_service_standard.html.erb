<% present service_standard do |service_standard_presenter| %>
  <div class="row collapse module-list-index <%= cycle('odd', 'even') %>" data-equalizer="module-list" id="item_<%= service_standard_presenter.id %>">
    <div class="large-1 hide-for-medium-down columns" data-equalizer-watch="module-list">
      <span class="drag-helper"></span>
    </div>

    <div class="large-7 small-8 columns" data-equalizer-watch="module-list">
      <%= service_standard_presenter.toggle_title %>
    </div>

    <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
      <%= service_standard_presenter.toggle_link %>
    </div>

    <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
      <%= service_standard_presenter.view_link %>
    </div>

    <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
      <%= service_standard_presenter.edit_link %>
    </div>

    <div class="large-1 hide-for-medium-down columns last" data-equalizer-watch="module-list">
      <%= service_standard_presenter.delete_link %>
    </div>

    <div class="row module-list-detail hide" id="index-list-<%= service_standard_presenter.id %>">
      <div class="small-12 columns module-list-summary last">
        <%= service_standard_presenter.optimadmin_summary %>
      </div>
    </div>
  </div>
<% end %>
