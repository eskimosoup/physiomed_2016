
<% present partner do |partner_presenter| %>
  <div class="row collapse module-list-index <%= cycle('odd', 'even') %>" id="item_<%= partner_presenter.id %>">
          <div class="large-1 hide-for-medium-down columns">
        <span class="drag-helper"></span>
      </div>
    
    <div class="columns">
      <%= partner_presenter.title %>
    </div>

          <div class="large-2 hide-for-medium-down columns">
        <%= partner_presenter.edit_image(:image) %>
      </div>
    
    <div class="large-1 small-4 text-center columns">
      <%= partner_presenter.toggle_link %>
    </div>

    <div class="large-1 small-4 text-center columns">
      <%= partner_presenter.view_link %>
    </div>

    <div class="large-1 small-4 text-center columns">
      <%= partner_presenter.edit_link %>
    </div>

    <div class="large-1 text-center columns last">
      <%= partner_presenter.delete_link %>
    </div>
  </div>
<% end %>

