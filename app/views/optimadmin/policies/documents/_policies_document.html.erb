
<% present policies_document do |policies_document_presenter| %>
  <div class="row collapse module-list-index <%= cycle('odd', 'even') %>" id="item_<%= policies_document_presenter.id %>">
          <div class="large-1 hide-for-medium-down columns">
        <span class="drag-helper"></span>
      </div>
    
    <div class="columns">
      <%= policies_document_presenter.title %>
    </div>

    
    <div class="large-1 small-4 text-center columns">
      <%= policies_document_presenter.toggle_link %>
    </div>

    <div class="large-1 small-4 text-center columns">
      <%= policies_document_presenter.view_link %>
    </div>

    <div class="large-1 small-4 text-center columns">
      <%= policies_document_presenter.edit_link %>
    </div>

    <div class="large-1 text-center columns last">
      <%= policies_document_presenter.delete_link %>
    </div>
  </div>
<% end %>

