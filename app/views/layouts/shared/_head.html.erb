<title><%= "#{@title} - " if @title %><%= global_site_settings['Name'] %></title>

<%= tag :meta, name: 'description', content: @meta_description if @meta_description %>
<%= tag :meta, name: 'keywords', content: @meta_tags if @meta_tags %>
<meta name="viewport" content="width=device-width, initial-scale=1">

<%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.css' %>
<%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick-theme.min.css' %>
<%= stylesheet_link_tag 'application', media: 'all' %>

<%= javascript_include_tag 'application' %>
<%= csrf_meta_tags %>
<%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js' %>
<%= javascript_include_tag 'cookie-consent/consent' %>

<%= yield :head %>

<%= render 'layouts/shared/remarketing_head' if Rails.env.production? %>

<script src="https://use.typekit.net/qmb4xev.js"></script>
<script>try{Typekit.load({ async: true });}catch(e){}</script>
